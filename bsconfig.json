{
  "name": "@karoshibee/re-tree",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": [
    "-bs-super-errors",
    "-open Belt"
  ],
  "warnings": {
    "error": "+8+101"
  },
  "namespace": true,
  "refmt": 3,
  "bs-dependencies": [
    "bs-log",
    "@glennsl/bs-json"
  ],
  "bs-dev-dependencies": [
    "bs-fast-check",
    "bs-mocha"
  ],
  "ppx-flags": [
    "bs-log/ppx"
  ]
}
